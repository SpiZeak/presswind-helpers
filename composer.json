{
  "name": "spizeak/press-wind-helpers",
  "description": "Helpers plugin for use with press-wind theme",
  "type": "wordpress-plugin",
  "homepage": "https://github.com/SpiZeak/press-wind-helpers",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "autoload": {
    "psr-4": {
      "PressWind\\": "src/"
    }
  },
  "authors": [
    {
      "name": "faramazpat",
      "email": "infos@goodmotion.fr"
    },
    {
      "name": "Max Trewhitt",
      "email": "max@trewhitt.se"
    }
  ],
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:SpiZeak/presswind-assets.git"
    }
  ],
  "require": {
    "php": ">=8.0",
    "spizeak/presswind-assets": "dev-main"
  }
}
